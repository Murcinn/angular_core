@model Insig.IdentityServer.Models.IndexViewModel

<div class="row">
    <div class="col-md-4">
        <form>
            <div class="form-group">
                <label asp-for="Username"></label>
                <input asp-for="Username" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="Email"></label>
                <input asp-for="Email" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="PhoneNumber"></label>
                <input asp-for="PhoneNumber" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="PhoneNumberConfirmed"></label>
                <input asp-for="PhoneNumberConfirmed" class="form-control checkbox-phone" disabled />
            </div>
        </form>
        <div class="mt-4 mb-4">
            <button class="btn btn-light" onclick="location.href = '@ViewBag.ReturnUrl'">Cancel</button>
        </div>
    </div>
</div>
